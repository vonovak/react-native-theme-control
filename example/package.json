{
  "name": "theme-expo-example",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "debugPrebuild": "EXPO_NO_GIT_STATUS=1 EXPO_DEBUG=1 expo prebuild --clean && xed ios",
    "web": "expo start --web",
    "debugLocal": "eas build --platform ios --profile development-simulator --local",
    "fix-deps": "expo install --check"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-menu/menu": "^1.2.3",
    "@react-native-segmented-control/segmented-control": "2.5.7",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@vonovak/react-native-theme-control": "portal:../",
    "expo": "^53",
    "expo-dev-client": "~5.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0"
  },
  "private": true
}
